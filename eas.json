{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": false,
      "distribution": "internal",
      "environment": "development",
      "android": {
        "buildType": "apk",
        "env": {
          "NODE_ENV": "development"
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "environment": "preview",
      "android": {
        "buildType": "apk",
        "env": {
          "NODE_ENV": "preview"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "environment": "production",
      "android": {
        "buildType": "apk",
        "env": {
          "NODE_ENV": "production"
        }
      }
    },
    "apk-release": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "env": {
          "NODE_ENV": "production",
          "EXPO_PUBLIC_HOST": "https://www.your-domain.com",
          "EXPO_PUBLIC_API_BASE_URL": "https://your-domain.com/api",
          "EXPO_PUBLIC_PROXY_HOST": "https://your-domain.com",
          "EXPO_PUBLIC_SENIVERSE_UUID": "your-uuid",
          "EXPO_PUBLIC_SENIVERSE_PRIVATE_KEY": "your-private-key",
          "EXPO_PUBLIC_DEEPSEEK_API_KEY": "your-api-key"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
